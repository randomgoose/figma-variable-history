{
  "name": "figma-variable-history-workspace",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "watch": "pnpm --dir packages/plugin watch",
    "build": "pnpm --dir packages/plugin build",
    "storybook": "pnpm --dir packages/stories storybook",
    "prepare": "husky install",
    "format": "prettier --config --write \"packages/**/*.{js,jsx,ts,tsx}\"",
    "eslint": "eslint packages/ --fix --cache --quiet --ext .js,.jsx,.ts,.tsx",
    "dev": "pnpm --dir packages/plugin dev"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/lodash-es": "^4.17.8",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
